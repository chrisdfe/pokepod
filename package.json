{
  "name": "pokepod",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "./node_modules/.bin/vue-cli-service serve",
    "build": "./node_modules/.bin/vue-cli-service build",
    "lint": "./node_modules/.bin/vue-cli-service lint",
    "release": "yarn && yarn build",
    "start": "./node_modules/.bin/http-server dist -p $PORT"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.21",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
